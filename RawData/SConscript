p1 = Program(source=['CalibrateOsm.cpp'], LIBS=['routingkit'])
p2 = Program(source=['ComputeDijkstraRanks.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p3 = Program(source=['ComputeDistances.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p4 = Program(
    source=['ConvertGraph.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj', 'routingkit'])
p5 = Program(source=['GenerateMatchingODPairs.cpp'], LIBS=['routingkit'])
p6 = Program(source=['GenerateODPairs.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p7 = Program(source=['ParseMobiTopp.cpp'], parse_flags='-DCSV_IO_NO_THREAD')
p8 = Program(source=['RadiationModel.cpp'], parse_flags='-DCSV_IO_NO_THREAD', LIBS=['proj'])

Alias('CalibrateOsm', p1)
Alias('ComputeDijkstraRanks', p2)
Alias('ComputeDistances', p3)
Alias('ConvertGraph', p4)
Alias('GenerateMatchingODPairs', p5)
Alias('GenerateODPairs', p6)
Alias('ParseMobiTopp', p7)
Alias('RadiationModel', p8)

Return('p1', 'p2', 'p3', 'p4', 'p5', 'p6', 'p7', 'p8')
